  lines:
  - speakerName: Hero
    line: Dang it I've been tricked!!
    voiceClip: {fileID: 8300000, guid: 957a8706b8d687a4db161b24ccd1a243, type: 3}
    leftSprite: {fileID: -6146700129834819302, guid: 9bcbe418df3b56e44ba75c64636feba5, type: 3}
