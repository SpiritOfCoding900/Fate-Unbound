    rightImageOpacity: 0
    centerImageOpacity: 1
  - speakerName: Civilian
    line: Oh no! Our Hero has failed us by being outsmarted by the enemy of humanity!
    voiceClip: {fileID: 8300000, guid: c0f25f09813fdd642baf46c93962d9ab, type: 3}
    leftSprite: {fileID: -6146700129834819302, guid: 9bcbe418df3b56e44ba75c64636feba5, type: 3}
